<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="servlets.toDoList" table="test">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="string">
            <column name="name" length="200" not-null="true" unique="true" />
        </property>
        <property name="name_do" type="string">
            <column name="name_do" length="200" not-null="true" unique="true" />
        </property>
        <property name="request" type="string">
            <column name="request" length="200" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>